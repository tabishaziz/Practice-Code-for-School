#include <iostream>
#include <cmath>
using namespace std;

int main(){

    cout<<"Radius =";
    
    int Radius;
    
    cin>> Radius;
    
    const double pi = 3.14;
    double AreaOfCircle = pi * pow(Radius, 2);
    
    cout <<"Area Of Circle = ";
    cout << AreaOfCircle;
    
    return 0;
    
    
}
