// program header starts
#include <iostream>
using namespace std;

//main function begins.
int main () {
    
    int x = 10;
    int y = 5;
    
    // In order to obtain accurate value in decimal point we will assign double variable to both the numerator and denominator values of the expression, so that our solution will be printed in decimal form.
    double a = x + 10;
    double b = 3 * y;
    
    float z = a / b;
    
   cout<<z;
    
    return 0;
}
